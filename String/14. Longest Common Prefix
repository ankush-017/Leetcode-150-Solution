class Solution {
    public String longestCommonPrefix(String[] strs) {
        
        String ans = "";
        int n = strs[0].length();
        for(int i=0;i<n;i++){
            char ch = strs[0].charAt(i);
            boolean flag = false;
            for(int j=0;j<strs.length;j++){
                if(strs[j].length()<=i || strs[j].charAt(i) != ch){
                    flag = true;
                    break;
                }
            }
            if(flag){
                break;
            }
            ans += ch;
        }

        return ans;
    }
}
