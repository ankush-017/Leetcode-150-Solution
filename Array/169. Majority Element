class Solution {
    public int majorityElement(int[] nums) {
        
        // TC: O(n)
        // SC: O(1)
        // approach: must greater that or equal to n/2
        int n = nums.length;
        int cnt = 0;
        int ele = 0;
        for(int i=0;i<n;i++){
            if(cnt == 0){
                ele = nums[i];
                cnt++;
            }
            else{
                if(nums[i] == ele){
                    cnt++;
                }
                else{
                    cnt--;
                }
            }
        }

        return ele;
    }
}
